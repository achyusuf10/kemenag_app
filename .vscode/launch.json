    "version": "0.2.0",
    "configurations": [
        {
            "name": "inisa dev",
            "request": "launch",
            "type": "dart",
            // "flutterMode": "release",
            "args": [
                "--flavor",
				"dev",
				"--target",
				"lib/main_dev.dart",
                "--no-sound-null-safety",
            ]
        },
        {
            "name": "inisa staging",
            "request": "launch",
            "type": "dart",
            // "flutterMode": "release",
            "args": [
                "--flavor",
				"qa",
				"--target",
				"lib/main_staging.dart",
                "--no-sound-null-safety",
            ]
        },
        {
            "name": "inisa",
            "request": "launch",
            "type": "dart",
            // "flutterMode": "release",
            "args": [
                "--flavor",
				"prod",
				"--target",
				"lib/main.dart",
                "--no-sound-null-safety",
            ]
        },
        {
            "name": "inisa (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        }
    ]
}